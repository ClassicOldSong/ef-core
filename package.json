    "prod": "cross-env NODE_ENV=production BUILD_TARGET=production rollup -c ./config/rollup.prod.js",
    "lint": "eslint --ext .js src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheNeuronProject/ef-core.git"
